{
  "name": "quickhost",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "migrate:init": "turbo run migration:generate && turbo run migration:run",
    "migrate:run": "turbo run migration:run"
  },
  "devDependencies": {
    "prettier": "^3.8.1",
    "turbo": "^2.7.5",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.8.0",
  "workspaces": [
    "apps/*"
  ]
}
